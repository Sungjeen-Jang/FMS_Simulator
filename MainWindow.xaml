<Window x:Class="FMS_Simulator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FMS_Simulator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Simulate" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Height="30" Click="OnSimulateClick" />
        <ListBox x:Name="lstResults" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,40,0,0" Width="480" Height="250"/>
    </Grid>
</Window>
